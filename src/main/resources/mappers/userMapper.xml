<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.onul.test.dao.UserDAO">
 	<select id="selectUserList" resultType="com.onul.test.model.User">
 		SELECT
 			`id`
			, `loginId`
			, `password`
			, `nickName`
			, `profileImage`
			, `name`
			, `email`
			, `createdAt`
			, `updatedAt`
		FROM
			`user`	
 	</select>
 </mapper>